<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <template id="subscription_order_report">
        <t t-call="web.html_container">
            <t t-foreach="docs" t-as="o">
                <t t-call="web.external_layout">
                    <div class="page">
                        <div class="text-center">
                            <h1>Subscription Orders</h1>
                        </div>
                        <h3>Subscription:
                            <span t-field="o.name"/>
                        </h3>
                    </div>
                </t>
            </t>
        </t>
    </template>
</odoo>