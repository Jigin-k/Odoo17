<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <template id="dynamic_snippets" inherit_id="website.snippets"
             name="Snippet">
       <xpath expr="//div[@id='snippet_effect']//t[@t-snippet][last()]"
              position="after">
           <t t-snippet="recurring_subscription.credit_snippet"/>
       </xpath>
   </template>
</odoo>