<templates id="credit_snippet_template" xml:space="preserve">
   <t t-name="recurring_subscription.subscription_credit_snippet_template">
       <div id="credits_cards_container" class="container">
           <div class="row">
               <t t-foreach="data" t-as="data" t-key="vals_index">
                   <a t-attf-href="/credits/{{data.id}}">
                       <div class="card">
                           <div class="card-body">
                               <h1 class="card-title" t-esc="data.name"/>
                               <h3 class="card-title" t-esc="data.credit_amount"/>
                           </div>
                       </div>
                   </a>
               </t>
           </div>
       </div>
   </t>
</templates>

